  },
  "dependencies": {
    "@vueuse/core": "^10.5.0",
    "core-js": "^3.8.3",
    "vue": "^3.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
