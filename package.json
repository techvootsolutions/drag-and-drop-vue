{
  "name": "@techvootsolutions/drag-and-drop-vue",
  "description": "Plugin for Drag and Drop",
  "version": "0.0.4",
  "private": false,
  "main": "dist/dragandropvue.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name dragandropvue src/index.js",
